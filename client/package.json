{
  "name": "survival-calculator",
  "version": "1.0.0",
  "main": "dist/electron/index.js",
  "scripts": {
    "dev": "concurrently \"yarn dev:vite\" \" yarn dev:electron\"",
    "dev:vite": "concurrently \"vite\" \"yarn watch-generate-types\"",
    "dev:electron": "yarn build:electron && electron ./dist/electron",
    "build": "yarn build:vite & yarn build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "serve": "vite preview",
    "clean": "rimraf dist",
    "type-check": "tsc",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "generate-types": "yarn tsm src --ignore \"!**/**.module.scss\" --logLevel silent",
    "watch-generate-types": "yarn chokidar **/**.module.scss -c \"yarn generate-types\" --initial --silent",
    "package": "yarn clean && yarn build && electron-builder"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/styles": "^4.11.4",
    "@mui/icons-material": "^5.0.5",
    "@mui/material": "^5.0.6",
    "@mui/styles": "^5.0.2",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@vitejs/plugin-react": "^1.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "axios": "^0.24.0",
    "downloadjs": "^1.4.7",
    "electron-context-menu": "^3.1.1",
    "electron-is-dev": "^2.0.0",
    "electron-window-state": "^5.0.3",
    "highcharts": "^9.3.0",
    "highcharts-react-official": "^3.1.0",
    "html-to-image": "^1.9.0",
    "lodash": "^4.17.21",
    "node-sass": "^6.0.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-loader-spinner": "^4.0.0",
    "react-router-dom": "^5.3.0",
    "rxjs": "^7.4.0",
    "use-async-effect": "^2.2.3",
    "vite": "^2.6.4",
    "vite-plugin-checker": "^0.3.4",
    "vite-plugin-eslint": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@types/babel__code-frame": "^7.0.3",
    "@types/babel__core": "^7.1.16",
    "@types/downloadjs": "^1.4.2",
    "@types/eslint": "^7.28.2",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.10.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^6.3.0",
    "electron": "^15.3.0",
    "electron-builder": "^22.13.1",
    "eslint": "^8.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "2.4.1",
    "sass": "^1.43.4",
    "typed-scss-modules": "^4.1.1",
    "typescript": "^4.3.2"
  },
  "build": {
    "productName": "Survival Calculator",
    "appId": "Survival.Calculator",
    "win": {
      "icon": "./public/WindowIcon.ico"
    },
    "files": [
      "dist/electron/**/*",
      "dist/vite/**/*",
      "public/**"
    ],
    "directories": {
      "output": "dist/release",
      "buildResources": "public"
    }
  },
  "license": "MIT"
}
